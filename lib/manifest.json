{
	"name": "Fanfiction.net usefull additions",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Adds usefull features to Fanfiction.net",
	"content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "jquery-1.7.2.min.js", "ffaddition.user.js" ],
        "matches":          [ "http://www.fanfiction.net/*",
       						  "http://fanfiction.net/*" ],
        "run_at": "document_end"
    } ]
}